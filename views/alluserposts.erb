
<ul>
<% @userpost.each do |userpost| %>
  <li>
  <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"> <%= userpost.title %> </span>
          <p> <h5 ><%= userpost.classification %></h5></p>
        </div>
        <div class="card-action">
         <p ><%= userpost.description %></p>
       <p> ID: <%= userpost.id %></p>
        </div>
      </div>
    </div>
  </div>
    </li>
<% end %>
</ul>       
