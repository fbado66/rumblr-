    <div class="col s12 m6">
      <div class="card">
        <div class="card-image">
          <img src="images/locker_room.jpg">
          <span class="card-title">SIGN UP</span>
        </div>
      </div>
    </div>
  </div>


<h2>Create an account</h2>
<div class="row">
  <div class="col s12 l6">
    <form action="/sign_up" method="post">
      <i class="material-icons prefix">account_circle</i>
    <input type="text" name="username" id="" placeholder="Username"><br><br>
      <i class="material-icons prefix">priority_high</i>
    <input type="password" name="password" id="" placeholder="Password"><br><br>
      <i class="material-icons prefix">person</i>
    <input type="text" name="first_name" id="" placeholder="First name"><br><br>
      <i class="material-icons prefix">person_outline</i>
    <input type="text" name="last_name" id="" placeholder="Last name"><br><br>
      <i class="material-icons prefix">mail</i> 
    <input type="text" name="email" id="" placeholder="123@provider.com"><br><br>
      Birthday <i class="material-icons prefix">cake</i> 
    <input type="date" name="birthday" id="" placeholder=""><br>
    <button class="btn waves-effect waves-light" type="submit" name="action">Sign Up
      <i class="material-icons right">send</i>
    </button>
   </form>
 </div>
</div>








        
  